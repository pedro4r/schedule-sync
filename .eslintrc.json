{
  "extends": [
    "@rocketseat/eslint-config/react",
    "next/core-web-vitals"
  ],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "endOfLine": "auto",
        "printWidth": 80,
        "trailingComma": "es5",
        "semi": false,
        "doubleQuote": true,
        "jsxSingleQuote": true,
        "singleQuote": true,
        "useTabs": false,
        "tabWidth": 4
      }
    ]
  }
}